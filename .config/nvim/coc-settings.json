{
  "diagnostic-languageserver.filetypes": {
    "sh": "shellcheck"
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "json",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "svelte",
    "php"
  ],
  "languageserver": {
    "ocaml-lsp": {
      "command": "opam",
      "args": ["config", "exec", "--", "ocamllsp"],
      "filetypes": ["ocaml", "reason"]
    }
  },
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "vue",
    "blade",
    "svelte"
  ],
  "tailwindCSS.trace.server": "verbose",
  "coc.source.vimtex.filetypes": ["tex", "plaintex", "latex", "markdown"]
}
